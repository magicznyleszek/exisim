{
  "name": "exisim",
  "version": "2.4.0",
  "description": "Zero-player human existence simulator",
  "author": {
    "name": "Leszek Pietrzak",
    "email": "leszek@magicznyleszek.xyz"
  },
  "private": false,
  "homepage": "https://github.com/magicznyleszek/exisim",
  "repository": "git+https://github.com/magicznyleszek/exisim.git",
  "bugs": "https://github.com/magicznyleszek/exisim/issues",
  "license": "CC0-1.0",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll --coverage",
    "start": "parcel -p 2038 src/index.html -d dev",
    "build-docs": "rm -rf docs && parcel build src/index.html -d docs --public-url ./ --no-cache && npm run copy-static-files",
    "copy-static-files": "cp CNAME favicon.png humans.txt LICENSE manifest.json favicon.ico favicon.png docs"
  },
  "devDependencies": {
    "@analytics/google-analytics": "^0.4.0",
    "@types/jest": "^25.2.2",
    "@types/lodash": "^4.14.151",
    "analytics": "^0.5.1",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
